type Paciente {
    id: ID!
    nome: String!
}

input PacienteRequestDto {
    nome: String!
}

type PacienteResponseDto {
    id: ID!
    nome: String!
}

type Query {
    consultarPacientePorId(id: ID!): PacienteResponseDto
}

type Mutation {
    criarPaciente(request: PacienteRequestDto): PacienteResponseDto
}

